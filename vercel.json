{
  "version": 3,
  "framework": "nest",
  "buildCommand": "nest build",
  "installCommand": "pnpm install --frozen-lockfile",
  "outputDirectory": "dist",
  "functions": {
    "api/index.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
